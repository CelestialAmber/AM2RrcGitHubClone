/// get_joyaxisname(axis_id)
if (argument0 == 0) return "X Axis";
if (argument0 == 1) return "Y Axis";
if (argument0 == 2) return "Z Axis";
if (argument0 == 3) return "R Axis";
if (argument0 == 4) return "U Axis";
if (argument0 == 5) return "V Axis";
if (argument0 == 6) return "POV"; // x-pov
if (argument0 == 7) return "POV"; // y-pov
